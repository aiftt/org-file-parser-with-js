{
  "name": "org-file-parser-with-js",
  "author": "gcclll <gccll.love@gmail.com>",
  "version": "0.1.31",
  "license": "MIT",
  "description": "A parser for Emacs Org mode files.",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./dist/parse.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gcclll/org-file-parser-with-js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "src/*"
      ]
    }
  },
  "keywords": [
    "emacs",
    "org-mode",
    "javascript",
    "parser",
    "typescript"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/org-file-parser-with-js.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/org-file-parser-with-js.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
